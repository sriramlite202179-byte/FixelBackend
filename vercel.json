{
    "functions": {
        "api/dist/main.py": {
            "runtime": "@vercel/python@4.6.0",
            "maxDuration": 60,
            "memory": 256
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/dist/main.py"
        },
        {
            "source": "/inngest",
            "destination": "/api/dist/main.py"
        },
        {
            "source": "/inngest/(.*)",
            "destination": "/api/dist/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "index.html"
        }
    ],
    "trailingSlash": false
}