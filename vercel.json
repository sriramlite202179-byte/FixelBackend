{
    "functions": {
        "api/dist/main.py": {
            "runtime": "@vercel/python@4.6.0",
            "maxDuration": 60,
            "memory": 256
        }
    },
    "devCommand": "cd frontend && yarn dev",
    "installCommand": "cd frontend && yarn install",
    "buildCommand": "cd frontend && yarn run build",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/dist/main.py"
        },
        {
            "source": "/inngest",
            "destination": "/api/dist/main.py"
        },
        {
            "source": "/inngest/(.*)",
            "destination": "/api/dist/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "trailingSlash": false,
    "outputDirectory": "frontend/dist"
}